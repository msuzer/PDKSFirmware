{
  "name": "Custom ESP32-S3 N16R8",
  "vendor": "Custom Vendor",
  "url": "https://custom-board.example.com",

  "frameworks": ["espidf"],

  "build": {
    "core": "esp32s3",
    "f_cpu": "240000000L",
    "f_flash": "80000000L",
    "flash_mode": "dio",

    "memory_type": "qio_opi",

    "psram_type": "opi",
    "psram_size": "8MB",

    "extra_flags": "-DARDUINO_ESP32S3_DEV"
  },

  "board_build": {
    "flash_size": "16MB"
  },

  "upload": {
    "maximum_size": 16777216,
    "maximum_ram_size": 524288,
    "speed": 921600,
    "require_upload_port": true
  },

  "debug": {
    "openocd_target": "esp32s3.cfg"
  },

  "connectivity": {
    "wifi": true,
    "bluetooth": true
  }
}
