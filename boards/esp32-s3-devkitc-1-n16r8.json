{
  "name": "Espressif ESP32-S3-DevKitC-1-N16R8",
  "vendor": "Espressif",
  "url": "https://docs.espressif.com/projects/esp-idf/en/latest/esp32s3/hw-reference/esp32s3/user-guide-devkitc-1.html",

  "frameworks": ["espidf"],

  "connectivity": ["wifi", "bluetooth"],

  "build": {
    "core": "esp32",
    "mcu": "esp32s3",
    "variant": "esp32s3",

    "f_cpu": "240000000L",
    "f_flash": "80000000L",
    "flash_mode": "qio",

    "memory_type": "qio_opi",
    "psram_type": "opi",
    "psram_size": "8MB",

    "extra_flags": [
      "-DCUSTOM_ESP32_S3_N16R8"
    ]
  },

  "board_build": {
    "flash_size": "16MB"
  },

  "upload": {
    "flash_size": "16MB",
    "maximum_size": 16777216,
    "maximum_ram_size": 524288,
    "require_upload_port": true,
    "speed": 921600
  },

  "debug": {
    "default_tool": "esp-builtin",
    "onboard_tools": ["esp-builtin"],
    "openocd_target": "esp32s3.cfg"
  }
}
